<h1>Log In</h1>
<%= form_for(:session, url: login_path) do |f| %>
  <% flash.each do |message_type, message| %>
    <div class="<%= message_type %>"><%= message %></div>
  <% end %>
<p> 
  <%= f.label :email %>
  <%= f.text_field :email %>
</p>
<p>
  <%= f.label :password %>
  <%= f.password_field :password %>
</p>
<p><%= f.label :remember_me  %>
   <%= f.check_box :remember_me %>
  </p>

<p><%= f.submit "Log In" %></p>
<% end %>