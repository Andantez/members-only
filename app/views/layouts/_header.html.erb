<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <h2><%= link_to "Members Only", root_path, id: "logo"%></h2>
      <nav class="nav-container">
          <ul class="nav navbar-nav navbar-right flex-row ul-container">
          <% if logged_in? %>
              <li class="list-item"><%= link_to "Post", new_post_path %> </li>
              <li class="list-item "><%= link_to "Log Out", logout_path, method: :delete %></li>
            <% else %>
              <li class="list-item "><%= link_to "Log In", login_path %></li>
            <% end %>
          </ul>
      </nav>
  </div>
</header>
